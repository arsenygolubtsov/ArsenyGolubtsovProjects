#include <iostream>
#include <sstream>
using namespace std;

bool readmatrix(int matrix[3][3])
{
	bool error = true;
	for (int i = 0; i<3; i++) {
		string string;
		getline(cin, string);
		istringstream stream(string);
		for (int j = 0; j < 3; ++j) {
			if (!(stream >> matrix[i][j])) {
				error = false;
			    cerr << "An error has occured while reading input data\n";return error;
				break;
		    }
		}
	}
	return error;
}

void add ( int matrix1[3][3], int matrix2[3][3], int matrix3[3][3] ) {
	for ( int i = 0; i < 3; i++ ) {
		for ( int j = 0; j < 3; j++ ) {
		    matrix3[i][j] = matrix1[i][j] + matrix2[i][j];
		}
	}
}

void sub ( int matrix1[3][3], int matrix2[3][3], int matrix3[3][3] ) {
	for ( int i = 0; i < 3; i++ ) {
		for ( int j = 0; j < 3; j++ ) {
		    matrix3[i][j] = matrix1[i][j] - matrix2[i][j];
		}
	}
}

void mult ( int matrix1[3][3], int matrix2[3][3], int matrix3[3][3] ) {
	for ( int i = 0; i < 3; i++ ) {
		for ( int j = 0; j < 3; j++ ) {
		    int result = 0;
		    for ( int k = 0; k < 3; k++ ) 
		    result += matrix1[i][k] * matrix2[k][j];
		    matrix3[i][j] = result;
		}
	}
}

void print ( int matrix3[3][3] ) {
    for ( int i = 0; i < 3; i++ ) {
		for ( int j = 0; j < 3; j++ ) {
             cout <<matrix3[i][j] << " ";
		}
		cout << endl;
	}
}

int main()
{
    int matrix1[3][3];
    int matrix2[3][3];
    int matrix3[3][3];
    string znak;
    if (readmatrix( matrix1 )) {
	getline( cin, znak );
	if (readmatrix( matrix2 )) {
		if ( znak == "+" ) {
			add(matrix1, matrix2, matrix3);
			print(matrix3);
		}
		    else if ( znak == "-" ) {
			sub(matrix1, matrix2, matrix3);
			print(matrix3);
		}	
	        else if ( znak == "*" ) {
			mult(matrix1, matrix2, matrix3);
			print(matrix3);
	        }
	    }
         }
    else {
		cerr << "An error has occured while reading input data\n";
    }
}		
