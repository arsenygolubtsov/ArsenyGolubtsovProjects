#include <iostream>
#include <sstream>

int main()
{
	int i, k, s, c, d;
	for (std::string string; std::getline(std::cin, string);) {
		int a[10];
		std::istringstream stream(string);
		bool error = false;
		for (int i = 0; i < 10; ++i) {
			if (!(stream >> a[i])) {
				error = true;
				break;
			}
		}

		for (std::string string; std::getline(std::cin, string);) {
			int b[10];
			std::istringstream stream(string);
			bool failure = false;
			for (int k = 0; k < 10; ++k) {
				if (!(stream >> b[k])) {
					failure = true;
					break;
				}
			}

			s = 0;
			for (i = 1; i < 10; ++i) {
				for (k = 1; k < 10; ++k) {
					if ((i == k) && ((a[i] + b[k]) > s)) s = a[i] + b[k];
				}
			}
			c = 0;
			for (k = 1; k < 10; ++k) {
				for (i = 1; i < 10; ++i) {
					if ((i == k - 1) && ((a[i] + b[k]) > c)) c = a[i] + b[k];
				}
			}

			if (s > c) d = s;
			if (s < c) d = c;
			if (!error) {
				std::cout << d << " " << std::endl;
			}
			else {
				std::cout << "An error has occured while reading numbers from line" << std::endl;
			}
		}
	}
}
