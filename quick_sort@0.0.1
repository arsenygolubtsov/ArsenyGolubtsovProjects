#include <iostream>
#include <sstream>
#include <string>
using namespace std;

void quick_sort(int * array, int first, int last) {
    int i = first;
    int j = last;
    int mid = array[(first+last) / 2];
     while (i <= j) {
        while (array[i] < mid) {
            i++;
        }
        while (array[j] > mid) {
            j--;
        }
        if (i <= j) {
          swap(array[i], array[j]);

            i++;
            j--;
        }
    } 
    if(j > first) {
        quick_sort(array, first, j);
    }
     if(last > i) {
        quick_sort(array, i, last);
    }
}

bool read(int *array, unsigned int size){
    string stroka;
    getline(cin, stroka);
    istringstream stream(stroka);
    for (unsigned int i = 0; i<size; ++i){
        if (!(stream >> array[i])){
            return false;
        }
    }
    return true;
}

int main(){
    unsigned int size;
    cin >> size;
    cin.get();
   int *array = new int[size];
    if (read(array, size)){
        quick_sort(array, 0, size-1);
        for (unsigned int i = 0; i<size; ++i)
            cout << array[i] << " ";
    }
    else {
        cout << "An error has occured while reading input data.";
    }
    delete[]array;
    return 0;
}
