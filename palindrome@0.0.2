#include <stdio.h>
#include <iostream>
#include <string>
using namespace std;

bool palindrom(string name) {
    int left_index = 0;
    int right_index = name.length()-1; 
    while(left_index < right_index){
        if( name[left_index++] != name[right_index--]){
            return false;
        }
    }
    return true;
}
string fixing(string name) {
    int len = name.length();
    for (int i = 0; i < len; i++) {
        name[i] = tolower(name[i]);
    }
    string namefixed;
    for (int i = 0; i < len; i++) {
        {
            if ((name[i] > 96) && (name[i] < 123)) { namefixed += name[i] - 32; }
        }
        {
            if ((name[i] > 64) && (name[i] < 91)) { namefixed += name[i]; }
        }
        {
            if ((name[i] > 47) && (name[i] < 58)) { namefixed += name[i]; }
        }
    }
    return namefixed;
}
int main()
{
    string name;
    getline(cin, name);
    cout << palindrom(fixing(name));
    cin.get();

}
