#include <iostream>
#include <sstream>
#include <string>

using namespace std;

int main()
{
	string string;
	int a, b, i=0;
	char znak;
	getline(cin, string);
	istringstream stream(string);
	if (!(stream >> a)){
	    int i=i+1;}
	else{
	while (stream >> znak) {
		stream >> b;
		if (znak == '+') a = a + b;
		else if (znak == '-') a = a - b;
		else if (znak == '*') a = a*b;
		else if (znak == '/') {
			if (b == 0) cout << "Error";
			else a = a / b;
		}
	}
	}
	if (i=1){cout<<"Error";}
}
