#include <iostream>
#include <sstream>
#include <string>

using namespace std;

int main()
{
	string string;
	int a, b;
	char znak;
	getline(cin, string);
	istringstream stream(string);
	if (!(stream >> a))
	{
	   cerr << "Error" << endl; return 0;
	}
	else {
	while (stream >> znak) {
	         if ((!(znak == '+')) || (!(znak == '-')) || (!(znak == '*')) || (!(znak == '/')))
	         {
	            cerr << "Error" << endl; return 0;
                 }
	      else {
                  if (!(stream >> b))
	          {
	              cerr << "Error" << endl; return 0;
                  }
             else {
	     if (znak == '+') a = a + b;
	     else if (znak == '-') a = a - b;
             else if (znak == '*') a = a*b;
	     else if (znak == '/') {
		     if (b == 0) cerr << "Error" << endl; 
		     else a = a / b;
	      }
           }
         }
       }
     }
     cout << a << endl;
}
