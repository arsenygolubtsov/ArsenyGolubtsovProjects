	#include <iostream>
#include <sstream>
#include <string>
 
using namespace std;
 
int main()
{
	string string;
	int a, b;
	char znak;
	getline(cin, string);
	istringstream stream(string);
	stream >> a;
	bool stop = false;
	while (stream >> znak) {
		stream >> b;
		if (znak == '+') a = a + b;
		else if (znak == '-') a = a - b;
		else if (znak == '*') a = a*b;
		else if (znak == '/') {
			if (b == 0) cout << "Error";
			else a = a / b;}
                else {stop = true; }
	             if (!stop) cout << a << endl; 
            else cout << "Error" << endl;
      }
}
 	

