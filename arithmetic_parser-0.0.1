#include <iostream>
using namespace std;

int main() {
	int a, b, c;
	char sign, sign1 = 0;
	cin >> a >> sign;
	if (sign == '>' || sign == '<') {
		cin >> sign1;
		cin >> b;
	}
	else cin >> b;
	if (sign == '+') {
		c = a + b;
	}
	else if (sign == '-') {
		c = a - b;
	}
	else if (sign == '*') {
		c = a*b;
	}
	else if (sign == '/') {
		if (b == 0) {
			cout << "Error";
			return -1;
		}
		c = a / b;
	}
	else if (sign == '%') {
		c = a%b;
	}
	else if (sign == '&') {
		c = a&b;
	}
	else if (sign == '|') {
		c = a | b;
	}
	else if (sign == '^') {
		c = a^b;
	}
	else if ((sign == '>') && (sign1 == '>')) {
		c = a >> b;
	}
	else if ((sign == '<') && (sign1 == '<')) {
		c = a << b;
	}
	else { cout << "Error"; return -1; }
	cout << "Result: " << c;
	{system("pause"); }

	return 0;
}
