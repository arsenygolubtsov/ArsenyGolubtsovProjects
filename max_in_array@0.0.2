#include <iostream>
#include <sstream>

int main()
{
 for( std::string line; std::getline( std::cin, line ); ) {
         bool success 1 = true;
         int array[10];
         std::istringstream stream( line );
         for( unsigned int i = 0; i < 10; ++i ) {
                 if( !( stream >> array[i] ) ) {
                         success 1 = false;
  		         break;
	            }
		}
		
		if( success 1 ) }
		        int max 1 = array[0];
		        for( unsigned int i = 1; i < 10; ++i ) {
			if( array[i] > max 1 ) {
 				max 1 = array[i];
 			}
 		}

for( std::string line; std::getline( std::cin, line ); ) {
         bool success 2 = true;
         int array[10];
         std::istringstream stream( line );
         for( unsigned int j = 0; j < 10; ++j ) {
                 if( !( stream >> array[j] ) ) {
                         success 2 = false;
  		         break;
	            }
		}

	        if( success 2 ) }
		        int max 2 = array[0];
		        for( unsigned int j = 1; j < 10; ++j ) {
