#include <iostream>
#include <sstream>

int main()
{
	int i, k, s, c, d;
	for (std::string string; std::getline(std::cin, string);) {
		int a[10];
		std::istringstream stream(string);
		bool error = false;
		for (int i = 0; i < 10; ++i) {
			if (!(stream >> a[i])) {
				error = true;
				break;
			}
		}

		for (std::string string; std::getline(std::cin, string);) {
			int b[10];
			std::istringstream stream(string);
			bool failure = false;
			for (int k = 0; k < 10; ++k) {
				if (!(stream >> b[k])) {
					failure = true;
					break;
				}
			}

			s = a[0];
			for (i = 1; i < 10; ++i) {
				if (s < a[i]) s = a[i];
			}
			c = b[0];
			for (k = 1; k < 10; ++k) {
				if (c < b[k]) c = b[k];
			}

			d = s + c;

			if ((!error) && (!failure)) {
				std::cout << d << " " << std::endl ;
			}
			else {
				std::cout << "An error has occured while reading numbers from line" << std::endl ;
			}
		}
	}
}
